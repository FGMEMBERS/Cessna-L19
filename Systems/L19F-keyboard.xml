<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <keyboard>
    
    <key n="100">
      <name>d</name>
      <desc>Crew door</desc>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>
    
    <key n="108">
      <name>l</name>
      <desc>Landing light</desc>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/landing-lights</property>
      </binding>
    </key>
    
    <key n="76">
      <name>L</name>
      <desc>Rudders</desc>
      <binding>
        <command>nasal</command>
        <script>doors.gouvernes.toggle();</script>
      </binding>
    </key>
    
  </keyboard>
  
</PropertyList>
