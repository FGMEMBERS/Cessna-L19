<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Based on http://www.warbirdalley.com/images/cockpits/O1-Birddog-cockpit-1000.jpg -->
<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- AI -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m> -0.088 </y-m>
      <z-m>  0.082 </z-m>
    </offsets>
  </model>
  
  <!-- ALTITUDE -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m> -0.003 </x-m>
      <y-m> -0.368 </y-m>
      <z-m> -0.103 </z-m>
    </offsets>
  </model>

  <!-- AMMETER -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/ammeter/ammeter.xml</path>
    <offsets>
      <x-m> 0.001 </x-m>
      <y-m> 0.244 </y-m>
      <z-m> 0.096 </z-m>
    </offsets>
  </model>
  
  <!-- ASI -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m> -0.003 </x-m>
      <y-m> -0.368 </y-m>
      <z-m> -0.013 </z-m>
    </offsets>
  </model>
  
  <!-- CLOCK -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m> -0.001 </x-m>
      <y-m> -0.120 </y-m>
      <z-m> -0.120 </z-m>
    </offsets>
  </model>
  
  <!-- COMPASS -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/compass/compass.xml</path>
    <offsets>
      <x-m>  0.002 </x-m>
      <y-m> -0.242 </y-m>
      <z-m>  0.026 </z-m>
    </offsets>
  </model>
  
  <!-- CLINDER TEMPERATURE -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/cyltemp/cyltemp.xml</path>
    <offsets>
      <x-m> -0.002 </x-m>
      <y-m>  0.061 </y-m>
      <z-m>  0.099 </z-m>
    </offsets>
  </model>
  
  <!-- FLAPS -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/flaps/flaps.xml</path>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.310 </y-m>
      <z-m> 0.080 </z-m>
    </offsets>
  </model>
  
  <!-- FUEL -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/fuel/fuel.xml</path>
    <offsets>
      <x-m> -0.002 </x-m>
      <y-m>  0.166 </y-m>
      <z-m>  0.093 </z-m>
    </offsets>
  </model>
  
  <!-- OIL -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/oil/oil.xml</path>
    <offsets>
      <x-m> -0.002 </x-m>
      <y-m>  0.232 </y-m>
      <z-m>  0.001 </z-m>
    </offsets>
  </model>
  
  <!-- OIL PRESSURE -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/oilpressure/oilpressure.xml</path>    <offsets>
      <x-m> -0.002 </x-m>
      <y-m>  0.140 </y-m>
      <z-m>  0.001 </z-m>
    </offsets>
  </model>
  
  <!-- RPM -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m> -0.003 </x-m>
      <y-m>  0.357 </y-m>
      <z-m> -0.092 </z-m>
    </offsets>
  </model>
  
  <!-- SUCTION -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/suction/suction.xml</path>
    <offsets>
      <x-m> -0.002 </x-m>
      <y-m>  0.050 </y-m>
      <z-m>  0.001 </z-m>
    </offsets>
  </model>
  
  <!-- THROTTLE -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/throttle/throttle.xml</path>
    <offsets>
      <x-m>  0.15376 </x-m>
      <y-m> -0.38200 </y-m>
      <z-m> -0.26880 </z-m>
      <roll-deg> 9 </roll-deg>
    </offsets>
  </model>
  
  <!-- TURN -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/turn/turn.xml</path>
    <offsets>
      <x-m> -0.001 </x-m>
      <y-m> -0.242 </y-m>
      <z-m> -0.106 </z-m>
    </offsets>
  </model>
  
  <!-- VSI -->
  <model>
    <path>Aircraft/Cessna-L19/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m> -0.003 </x-m>
      <y-m> -0.100 </y-m>
      <z-m> -0.048 </z-m>
    </offsets>
  </model>
  
</PropertyList>
